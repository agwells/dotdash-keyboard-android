<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/cheatsheettable" >

    <TableRow
        android:id="@+id/tableRow1"
        style="@style/cheatsheettoprow" >

        <TextView
            style="@style/symbol"
            android:text="A" />

        <TextView
            style="@style/code"
            android:text=". -" />

        <TextView
            style="@style/symbol"
            android:text="N" />

        <TextView
            style="@style/code"
            android:text="- ." />

        <TextView
            style="@style/symbol"
            android:text="1" />

        <TextView
            style="@style/code_nopadding"
            android:text=". - - - -" />
    </TableRow>

