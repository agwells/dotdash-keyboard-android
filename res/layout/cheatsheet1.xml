    <TableRow style="@style/cheatsheetrow" >

        <TextView
            style="@style/symbol"
            android:text="E" />

        <TextView
            style="@style/code"
            android:text="." />

        <TextView
            style="@style/symbol"
            android:text="R" />

        <TextView
            style="@style/code"
            android:text=". - ." />

        <TextView
            style="@style/symbol"
            android:text="5" />

        <TextView
            style="@style/code_nopadding"
            android:text=". . . . ." />
    </TableRow>

    <TableRow style="@style/cheatsheetrow" >

