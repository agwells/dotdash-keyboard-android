        <TextView
            style="@style/symbol"
            android:text="P" />

        <TextView
            style="@style/code"
            android:text=". - - ." />

        <TextView
            style="@style/symbol"
            android:text="3" />

        <TextView
            style="@style/code_nopadding"
            android:text=". . . - -" />
    </TableRow>

    <TableRow style="@style/cheatsheetrow" >

        <TextView
            style="@style/symbol"
            android:text="D" />

        <TextView
            style="@style/code"
            android:text="- . ." />

        <TextView
            style="@style/symbol"
            android:text="Q" />

        <TextView
            style="@style/code"
            android:text="- - . -" />

        <TextView
            style="@style/symbol"
            android:text="4" />

        <TextView
            style="@style/code_nopadding"
            android:text=". . . . -" />
